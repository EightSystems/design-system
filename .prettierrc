{
    "arrowParens": "avoid",
    "editor.formatOnSave": true,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": true,
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "printWidth": 100,
    "proseWrap": "always",
    "requirePragma": false,
    "semi": false,
    "semicolons": true,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "es5",
    "useTabs": false
}
